{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Symphony",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#numliterals"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.symphony",
				"match": "\\b(if-goto|goto|return|label)\\b"
			}]
		},
		"functions":{
			"patterns": [{
				"name":"keyword.control.symphony",
				"match": "\\b(function|call)\\s+([^ \\/]*)",
				"captures": {
					"2":{
						"patterns": [{
							"name":"entity.name.function",
							"match": "[a-zA-Z\\_\\.\\$\\:]+[a-zA-Z\\_\\.\\$\\:\\d]*"
						}]
					}
				}

			}]
		},
		"numliterals":{
			"patterns": [{
				"name":"constant.numeric",
				"match": "\\d+"
			}]
		},
		"strings": {
			"name": "string.quoted.double.symphony",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.symphony",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.symphony"
	
}